<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.andriodapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/laucher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" android:persistent="true">
        <activity
            android:name="com.example.andriodapp.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.example.andriodapp.FpsActivity"
            android:label="@string/app_name" >
        </activity>
        <receiver android:name="com.example.androidapp.reciever.TestReciever"></receiver>
        <service android:name=".service.TestService"></service>
    </application>
  <uses-permission android:name="android.permission.INTERNET" />
</manifest>